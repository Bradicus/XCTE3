<?xml version="1.0" encoding="utf-8"?>

<!-- This file defines includes that automatically get added
     to a class file if a variable of a certain type is encountered
 -->
 
<xcte:AUTOINCLUDES>
    <xcte:INCL name="&lt;string&gt;">
		<xcte:VAR_TYPE name="string" />
	</xcte:INCL>
	
	<xcte:INCL name="&lt;iostream&gt;">
		<xcte:VAR_TYPE name="std::stringstream" />
	</xcte:INCL>
</xcte:AUTOINCLUDES>