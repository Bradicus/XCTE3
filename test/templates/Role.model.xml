<?xml version="1.0" encoding="utf-8"?>
<model name="role">

	<description>
	</description>

	<var_group genGet="true" genSet="true" visibility="private">
		<var name="id" identity="(1,1)" type="Int64" maxlen="10" pkey="true" />
		<var name="name" type="String" maxlen="50" />
		<var name="description" type="String" />
	</var_group>

		<class_group name="data store">
		</class_group>

		<derive model_set="edit">
			<class_group name="edit" feature_group="role" />
		</derive>

		<derive model_set="listing">
			<class_group name="listing" feature_group="role" />
		</derive>

		<gen_class type="class_mapper_derived" path="mapper">
			<derived_class name="admin edit" plugin="standard" />
			<data_class name="admin" plugin="{DataModelPlugin}" />
		</gen_class>

</model>
