<?xml version="1.0" encoding="utf-8"?>
<model name="address">

	<description>
	</description>

	<var_group genGet="true" genSet="true" visibility="private">
		<var name="id"	identity="(1,1)"	type="Int64" pkey="true" />
		<var name="street 1"	type="String" maxlen="50" required="true" />
		<var name="street 2"	type="String" maxlen="50" />
		<var name="city"	type="String" maxlen="50" required="true" />
		<var name="state"	type="String" maxlen="2" required="true" />
		<var name="country"	type="String" maxlen="2" required="true" />
		<var name="zip code"		type="String" maxlen="20" required="true" />
	</var_group>

	<class_group name="data store">
	</class_group>

	<derive model_set="edit">
		<class_group name="edit" />
	</derive>

	<derive model_set="listing">
		<class_group name="listing" />
	</derive>

	<gen_class type="class_mapper_derived">
		<derived_class name="admin edit" plugin="standard" />
		<data_class name="admin" plugin="$DataModelPlugin" />
	</gen_class>

</model>
