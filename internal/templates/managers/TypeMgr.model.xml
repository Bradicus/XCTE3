<?xml version="1.0" encoding="utf-8"?>
<model name="type mgr" >

	<description>
	Type Manager
	</description>

	<var_group genGet="true" genSet="true" init_vars="true">		
		<var name="types"		    utype="type" static="true" set="List" />		
	</var_group>

	<gen_class type="class_standard" path="managers">
    
		<empty_function name="in category" >
			<parameters>
				<var name="dataVar"	    utype="code elem variable" />
				<var name="category"	type="String" />
			</parameters>
		</empty_function>
	</gen_class>

</model>
