<?xml version="1.0" encoding="utf-8"?>
<model name="class spec mgr" >

	<description>
	Class Spec Manager
	</description>

	<var_group genGet="true" genSet="true" init_vars="true">		
		<var name="items"		    utype="type" static="true" set="List" />		
	</var_group>

	<gen_class type="class_standard" path="managers">
    
		<empty_function name="find class" >
			<parameters>
				<var name="data spec name"	type="String" />
				<var name="plugin name"		type="String" />
			</parameters>
		</empty_function>
	</gen_class>

</model>
