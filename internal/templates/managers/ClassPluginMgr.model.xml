<?xml version="1.0" encoding="utf-8"?>
<model name="class plugin mgr" >

	<description>
	Class Plugin Manager
	</description>

	<var_group genGet="true" genSet="true" init_vars="true">		
		<var name="types"		    utype="type" set="List" />		
	</var_group>

	<gen_class type="standard" path="managers">
    
		<empty_function name="find plugin" >
			<parameters>
				<var name="plugin name"		type="String" />
			</parameters>
		</empty_function>
	</gen_class>

</model>
