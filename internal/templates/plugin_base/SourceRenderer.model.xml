<?xml version="1.0" encoding="utf-8"?>
<model name="source renderer" >

	<description>
	Parameters to each val function in utilities
	</description>

	<gen_class type="standard" ns="plugin base" path="plugin base">
	</gen_class>

	<var_group genGet="true" genSet="true" init_vars="true">
		<var name="indent level"	type="Int32" default="0" />
		<var name="indent chars"	type="String" default="    " />
		<var name="lines"	type="String" set="list" />
		<var name="block delim open"	type="String" default="{" />
		<var name="block delim close"	type="String" default="}" />
		<var name="hanging function start"		type="Boolean" default="false" />
		<var name="hanging block start"		type="Boolean" default="true" />
		<var name="line ending"	type="String" default="\n" />
	</var_group>

</model>
